{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "JwtSettings": {
    "Secret": "secret",
    "AccessTokenExpiryMinutes": 60,
    "RefreshTokenExpiryMinutes": 43200,
    "Issuer": "Triggo Api",
    "Audience": "Triggo Client"
  },
  "UserSettings": {
    "DefaultPicture": "https://play.nintendo.com/images/profile-mk-kamek.7bf2a8f2.aead314d58b63e27.png"
  },
  "ConnectionStrings": {
    "DefaultDatabase": "Host=myserver;Username=mylogin;Password=mypass;Port=5432;Database=mydatabase"
  },
  "IntegrationsSettings": {
    "Discord": {
      "ApiEndpoint": "https://discord.com/api/v10",
      "OAuth2Endpoint": "https://discord.com/oauth2/authorize",
      "ClientId": "clientId",
      "ClientSecret": "clientSecret",
      "RedirectUrl": "redirectUrl",
      "Scope": [
        "identify",
        "email",
        "gdm.join",
        "guilds",
        "guilds.join",
        "role_connections.write"
      ]
    }
  }
}
